<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/spinkhead %>

  </head>
  <body>
    <header>
      <% include ../partials/spinkheader %>
    </header>
    <div class="users" style="width:100%; max-width: 600px; margin: 0 auto;">
      <!-- main start -->

<!-- views/users/new.ejs -->
<div class="w3-container">
  <h2></h2>
  <p></p>


  <div class="w3-card-4">
    <header class="w3-container w3-blue">
      <div class="wrapper1">
        <div class="contents1"><h3>JOIN</h3></div>
        <div class="sidebar1"><button type="button" class="btn btn-link" onclick="location.href='/spink'"><p style="color:white; padding-top:4px; margin-bottom: 0px;">back</p></button></div>
      </div>
    </header>
      <div class="w3-container">




            <form class="checkValidation" action="/users" method="post">
              <div class="w3-container">
              <br>
              <label>Email*</label>
              <input class="cvRequired w3-input" type="text" name="user[email]" cvRqErrMsg="Email is required." cvRqErrTo="#cvErrEmail" value="<%= formData?formData.email:"" %>">
              <div id="cvErrEmail" class="message"><%= emailError?emailError:"" %></div>
              <br>
              <label>Nickname*</label>
              <input class="cvRequired w3-input" type="text" name="user[nickname]" cvRqErrMsg="Nickname is required." cvRqErrTo="#cvErrNickname" value="<%= formData?formData.nickname:"" %>">
              <div id="cvErrNickname" class="message"><%= nicknameError?nicknameError:"" %></div>
              <br>
              <label>Password*</label>
              <input class="cvRequired w3-input" type="text"type="password" name="user[password]" cvRqErrMsg="Password is required." cvRqErrTo="#cvErrPassword"
                cvMatW="#cvMatWPassword" cvMatErrMsg="Password confirmation doesn't match." cvMatErrTo="#cvErrPasswordConfirmation" value="">
                <div id="cvErrPassword" class="message"><%= passwordError?passwordError:"" %></div>
              <br>
              <label>Password confirmation*</label>
              <input class="cvRequired w3-input" type="text"name="user[passwordConfirmation]" value="">
              <div id="cvErrPasswordConfirmation" class="message"></div>
              <p><small>*Required</small></p>
              <p>
                <div class="singleWrap">
                  <div class="contents"><h2><small></small></h2></div>
                  <div class="singleSide" style="float: right;"><button type="submit" style="width:60pt" class="btn btn-default">Submit</button></div>
                </div>
              </div>
            </form>

  </div>  <!--<div class="w3-container">-->
</div>  <!--w3-card-4 end-->

  <div class="w3-container w3-blue">
    <div style="float: right; margin-right: 10px; height:30pt; display:inline-block"></div>
  </div>
</div>
<!-- main end -->
</div>
<footer>
  <% include ../partials/spinkfooter %>
</footer>

</body>
</html>
